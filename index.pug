doctype html
html(lang='en')
  head
    link(rel="icon" href="Images/vsLogo.png")
    link(rel='apple-touch-icon-precomposed', size='57x57' href='Images/vsLogo57x57.png')
    meta(charset='UTF-8')
    meta(name='viewport', content='width=device-width, initial-scale=1.0')
    meta(http-equiv='X-UA-Compatible', content='ie=edge')
    script(src='https://www.gstatic.com/firebasejs/4.3.0/firebase.js')
    script(src='https://cdn.plot.ly/plotly-latest.min.js')
    script(src='https://cdn.jsdelivr.net/npm/clipboard@1/dist/clipboard.min.js')
    link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css')
    //link(rel='stylesheet', href='reset.css')
    link(rel='stylesheet', href='css/styles.css')
    title VS
  body
    .main-wrapper
      #overlay
        p.overlayText
      nav.navbar

        //#mobileFolder.option-folder
        //  button#mobileBtn.folder-toggle.fa.fa-bars
        //  .dropdown.hidden
        //    button#ladder.mobileBtn.optionBtn.folderBtn Overview
        //    button#power.mobileBtn.optionBtn.folderBtn Tier List
        //    button#table.mobileBtn.optionBtn.folderBtn Matchups
        //    button#decks.mobileBtn.optionBtn.folderBtn Decks
        //    button#info.mobileBtn.optionBtn.folderBtn Info
        .mobileTabs
          button.tab Tab Title
        .tabs
          button#ladder.tab.highlighted Overview
          button#power.tab Tier List
          button#table.tab Matchups
          button#decks.tab Decks
          button#info.tab info
        #updateTimeDiv.tooltip
          p#updateTime
          span.tooltipText Last updated
        #vsLogoDiv
          img(src="Images/vsGoldLogo.png").logo
        //.refreshArrow.fa.fa-refresh
      .content-wrapper
        #loader

        #ladderWindow.tabWindow
          .content-header
            .ftrOptions.optionBar
              #formatFolder.option-folder
                button#formatBtn.folder-toggle Standard
                .dropdown.hidden
              #timeFolder.option-folder
                button#timeBtn.folder-toggle Last Day
                .dropdown.hidden
              #rankFolder.option-folder
                button#rankBtn.folder-toggle All Ranks
                .dropdown#rankDropdown.hidden              
            .classDeckOptions.optionBar
              button#classes.optionBtn Classes
              button#decks.optionBtn Decks
              button#showNumbers.optionBtn.fa.fa-hashtag.tooltip
                span.tooltipText Show Numbers
            #plotFolder.option-folder
              button#plotBtn.folder-toggle.fa.fa-bar-chart
              .dropdown#plotDropdown.hidden
                button#bar.optionBtn.folderBtn.fa.fa-bar-chart
                button#line.optionBtn.folderBtn.fa.fa-line-chart
                button#pie.optionBtn.folderBtn.fa.fa-pie-chart
                button#number.optionBtn.folderBtn.fa.fa-th
                button#timeline.optionBtn.folderBtn.fa.fa-hourglass
            .graphOptions.optionBar
              button#bar.optionBtn.fa.fa-bar-chart.tooltip
                span.tooltipText Show Bar Chart
              button#line.optionBtn.fa.fa-line-chart.tooltip
                  span.tooltipText Show Line Chart
              button#pie.optionBtn.fa.fa-pie-chart.tooltip
                  span.tooltipText Show Pie Chart
              button#number.optionBtn.fa.fa-th.tooltip
                  span.tooltipText Show Table
              button#map.optionBtn.fa.fa-map.tooltip
                  span.tooltipText Show Power Map
              button#timeline.optionBtn.fa.fa-hourglass.tooltip
                  span.tooltipText Show Timeline
            .shadow
          .overlay
            p.overlayText
          .content
            p.graphTitle.chart-header
            .question.fa.fa-question-circle
            p.graphLabel
            #chart1.chart
            .chart-footer
          .content-footer Copyright (vS) Vicious Syndicate Gaming - www.ViciousSyndicate.com - © 2016-2018. All rights reserved.
          
        #powerWindow.tabWindow
          .content-header
            .formatOptions.optionBar
              button#Standard.optionBtn Standard
              button#Wild.optionBtn Wild
            .dispOptions.optionBar
              button#brackets.optionBtn Tier Lists
              button#ranks.optionBtn Ranks
            .shadow
          .overlay
            p.overlayText
          .content
            .question.fa.fa-question-circle
            #powerGrid.chart
            .chart-footer
          .content-footer Copyright (vS) Vicious Syndicate Gaming - www.ViciousSyndicate.com - © 2016-2018. All rights reserved.
          
            
        #tableWindow.tabWindow
          .content-header
            .ftrOptions.optionBar
              #formatFolder.option-folder
                button#formatBtn.folder-toggle Standard
                .dropdown.hidden
              #timeFolder.option-folder
                button#timeBtn.folder-toggle Last Month
                .dropdown.hidden
              #rankFolder.option-folder
                button#ranksBtn.folder-toggle All Ranks
                .dropdown.hidden
              p.nrGames              
            .graphOptions.optionBar
              p Sort 
              #sortFolder.option-folder
                button#sortBtn.folder-toggle Sort
                .dropdown.hidden
              button#changeColor.optionBtn.fa.fa-paint-brush.tooltip
                span.tooltipText Change Color
              button#showNumbers.optionBtn.fa.fa-hashtag.tooltip
                span.tooltipText Show Numbers
            .shadow
          .overlay
            p.overlayText
          .content
            .question.fa.fa-question-circle
            #chart2.chart
            .chart-footer
              //.colorTheme.chartFooterBtn
              //  button.changeColorBtn Change Colors
              .equilibrium.chartFooterBtn
                button.equilibriumBtn Simulation
                  .fa.fa-rocket
              .download.chartFooterBtn
                button.downloadBtn Download Table
                  .fa.fa-cloud-download
          .content-footer Copyright (vS) Vicious Syndicate Gaming - www.ViciousSyndicate.com - © 2016-2018. All rights reserved.

        #decksWindow.tabWindow
          .content-header
            .ftrOptions.optionBar
              #formatFolder.option-folder
                button#formatBtn.folder-toggle Standard
                .dropdown.hidden
                  button#Standard.optionBtn.folderBtn Standard
                  button#Wild.optionBtn.folderBtn Wild
              button#description.optionBtn Text
              button#decklists.optionBtn Decks
              button#dustWr.optionBtn Dust
              #classFolder.option-folder
                button#classBtn.folder-toggle Druid
                .dropdown.hidden
                  button.optionBtn.folderBtn Classes
                  button.optionBtn.folderBtn Meta
                  button.optionBtn.folderBtn Special
                  button#Druid.optionBtn.folderBtn Druid 
                  button#Hunter.optionBtn.folderBtn Hunter 
                  button#Mage.optionBtn.folderBtn Mage
                  button#Paladin.optionBtn.folderBtn Paladin
                  button#Priest.optionBtn.folderBtn Priest
                  button#Rogue.optionBtn.folderBtn Rogue
                  button#Shaman.optionBtn.folderBtn Shaman
                  button#Warlock.optionBtn.folderBtn Warlock
                  button#Warrior.optionBtn.folderBtn Warrior
            .displayOptions.optionBar
              button#info.optionBtn Info
              button#compare.optionBtn Compare
            .shadow
          .overlay
            p.overlayText
          .content
            .question.fa.fa-question-circle
            .content-left
              .selectionWrapper
                //.imgWrapper
                //  img(src="").selectionImg
                //  p.selectionTitle Hunter
                .buttonWrapper
              .sidebar.left
            .content-middle
              .descriptionBox
                .description
              .decklists
              #chart3.chart
            //.content-right
            //  .sidebar.right1
            //  .sidebar.right2
          .content-footer Copyright (vS) Vicious Syndicate Gaming - www.ViciousSyndicate.com - © 2016-2018. All rights reserved.
            
          
        #infoWindow.tabWindow
          .content-header
            .graphOptions.optionBar
              button#info.optionBtn.fa.fa-info.tooltip
                span.tooltipText Information
              button#twitter.optionBtn.fa.fa-twitter.tooltip
                  span.tooltipText Twitter Feed
            .shadow
          .content
            .infoText
            .twitterDiv
              //a( class='twitter-timeline' href='https://twitter.com/ViciousHS?ref_src=twsrc%5Etfw>Tweets by ViciousHS' )
